{
  "name": "caption-highlighter",
  "displayName": "Caption Highlighter",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "description": "Chrome extension that highlights Google Meet captions using a custom dictionary and tooltips.",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:prepare dev:background dev:web dev:devpage dev:js dev:gemini",
    "dev-firefox": "npm run clear && cross-env NODE_ENV=development EXTENSION=firefox run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite",
    "dev:devpage": "vite --config vite.config.dev.mts",
    "dev:js": "npm run build:js -- --mode development",
    "dev:gemini": "npm run build:gemini -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:background build:js build:gemini",
    "build:gemini": "vite build --config vite.config.gemini.mts",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.mts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.mts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "lint": "eslint --cache .",
    "test": "vitest test",
    "test:e2e": "playwright test",
    "postinstall": "simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@vueuse/core": "^11.0.1",
    "papaparse": "^5.4.1",
    "pinia": "^2.1.7",
    "vue": "^3.5.8",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.0",
    "@ffflorian/jszip-cli": "^3.8.5",
    "@iconify/json": "^2.2.239",
    "@playwright/test": "^1.46.1",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-interactions": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@storybook/vue3-vite": "^8.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.0",
    "@types/webextension-polyfill": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@unocss/reset": "^0.62.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/compiler-sfc": "^3.5.8",
    "@vue/test-utils": "^2.4.6",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^9.9.0",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.1.1",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^8.2.4",
    "typescript": "^5.5.4",
    "unocss": "^0.62.2",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-icons": "^0.19.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5",
    "vue-demi": "^0.14.10",
    "web-ext": "^8.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
